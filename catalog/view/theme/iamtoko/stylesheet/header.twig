<header>
    <div class="relative">
      <div class="scrollup">
        <!-- Иконка fa-chevron-up (Font Awesome) -->
        <i class="fa fa-chevron-up"></i>
      </div>
    </div>
  <div class="container-fluid t-header">
    <div class="row bad">
        <div class="col-md-2 hidden-xs hidden-sm"><a href="{{ manufacture_top }}">{{ text_manufacture_top }}</a></div>
        <div class="col-md-2 hidden-xs hidden-sm"><a href="{{ about_us_toko }}">{{ text_online_top }}</a></div>
        <div class="col-md-2 hidden-xs hidden-sm"><a href="{{ delivery_toko }}">{{ text_exchange_top }}</a></div>
        <div class="col-md-2 hidden-xs hidden-sm"><a href="{{ delivery_toko }}">{{ text_payment_top }}</a></div>
        <div class="col-md-2 hidden-xs hidden-sm"><a href="/index.php?route=extension/module/p_review/all">{{ text_reviewus_top }} <span>☺</span></a></div>
        <div class="col-md-2 hidden-xs hidden-sm bad3">{{ text_contactus_top }}:<a href="tel:{{ telephone_href }}" title="Позвонить">{{ telephone }}</a></div>
    </div>
  </div>
  <div class="line"></div>
  <div class="container-fluid header">
    <div class="row">
        <div class="col-md-2 hidden-sm hidden-xs"></div>
        <div class="col-md-2 col-sm-4 col-xs-12">
        {{ menu }}
          <div id="logo" class="logo">
            <a href="{{ home }}"><span>iam</span>TOKO
            </a>
          </div>
        </div>
        <div class="col-md-4 col-sm-3 col-xs-12">{{ search }}</div>
        <div class="col-md-4 col-sm-5 col-xs-12 menu-right">

            <ul class="list-inline hed-menu">

              <li class="dropdown"><a href="{{ account }}" title="{{ text_account }}" class="dropdown-toggle" data-toggle="dropdown">
                  <i>
                      <svg class="icon" viewBox="0 0 25 25" width="25" height="25">
                          <use xmlns:xlink="https://www.w3.org/1999/xlink" xlink:href="image\sprite.svg#icon-login"></use>
                      </svg>
                  </i>
                <ul class="dropdown-menu dropdown-menu-right">
                  {% if logged %}
                  <li class="login-name">{{ LastName }} {{ FirstName }}</li>
                  <li><a href="{{ account }}">{{ text_account }}</a></li>
                  <li><a href="{{ order }}">{{ text_order }}</a></li>
                  <li><a href="{{ transaction }}">{{ text_transaction }}</a></li>
                  <li><a href="{{ download }}">{{ text_download }}</a></li>
                  <li><a href="{{ logout }}">{{ text_logout }}</a></li>
                  {% else %}
                  <li><a href="{{ register }}">{{ text_register }}</a></li>
                  <li><a href="{{ login }}">{{ text_login }}</a></li>
                  {% endif %}
                </ul>
              </li>
              <li class="phone-xs dropdown">
                  <a href="{{ contact }}" title="{{ text_contact }}" class="dropdown-toggle" data-toggle="dropdown">
                      <i>
                          <svg class="icon {{ wl_class }}" viewBox="0 0 27 27" width="27" height="27">
                              <use xmlns:xlink="https://www.w3.org/1999/xlink" xlink:href="image\sprite.svg#icon-phone"></use>
                          </svg>
                      </i>
                  </a>
                  <span class="hidden-xs hidden-sm hidden-md">{{ telephone }}</span>
                  <ul class="dropdown-menu dropdown-menu-center drop-phone-xs">
                      <li><a href="#modalCallbackFooterFooter" id="button_callbackfooter" data-toggle="modal"><i class="fa fa-volume-control-phone"></i> <span>{{ text_button_callback }}</span></a></li>
                      <li><a href="tel:{{ telephone_href }}" title="Позвонить">{{ telephone }}</a></li>
                      <li><a href="tel:{{ telephone_href2 }}" title="Позвонить">{{ telephone2 }}</a></li>
                      <li><a href="mailto:{{ email }}" title="email">{{ email }}</a></li>
                      <li>
                          <ul class="social-mi">
                              <li>
                                <a href="viber://chat?number={{ viber }}" data-hover="hover-on-link" title="viber">
                                    <svg class="icon icon-viber" viewBox="0 0 30 30" width="30" height="30">
                                      <use xmlns:xlink="https://www.w3.org/1999/xlink" xlink:href="image\sprite.svg#icon-viber"></use>
                                    </svg>
                                </a>
                              </li>
                              <li>
                                <a href="tg://resolve?domain={{ telegram }}">
                                  <svg class="icon icon-telegram" viewBox="0 0 30 30" width="30" height="30">
                                    <use xmlns:xlink="https://www.w3.org/1999/xlink" xlink:href="image\sprite.svg#icon-telegram"></use>
                                  </svg>
                                </a>
                              </li>
                          </ul>
                      </li>
                  </ul>
              </li>
              <li><a href="{{ wishlist }}" id="wishlist-total" title="{{ text_wishlist }}">
                  <i>
                      <svg class="icon {{ wl_class }}" viewBox="0 0 26 25" width="26" height="25">
                          <use xmlns:xlink="https://www.w3.org/1999/xlink" xlink:href="image\sprite.svg#icon-heart"></use>
                      </svg>
                  </i>
                  <span class="hidden-xs hidden-sm hidden-md">{{ text_wishlist }}</span></a>
              </li>
              <li>{{ cart }}</li>
              <li>{{ language }}</li>
            </ul>
        </div>
    </div>
    {{ tmdheader }}
  </div>
  <input type="hidden" value="{{ one_click }}" id="one_click" />
  <input type="hidden" value="{{ name_callback }}" id="name_callback" />
  <input type="hidden" value="{{ phone_callback }}" id="phone_callback" />
  <input type="hidden" value="{{ entry_name }}" id="entry_name" />
  <input type="hidden" value="{{ entry_phone }}" id="entry_phone" />
  <input type="hidden" value="{{ entry_enquiry }}" id="entry_enquiry" />
  <input type="hidden" value="{{ text_send }}" id="text_send" />
  <input type="hidden" value="{{ text_loading }}" id="text_loading" />
  <input type="hidden" value="{{ success_message }}" id="success_message" />


</header>
